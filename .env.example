# VTEX Navigation CLI Configuration
# Copy this file to .env and customize as needed

# Default navigation URL (optional)
# If not set, defaults to https://newhelp.vtex.com/navigation.json
VTEX_NAVIGATION_URL=https://newhelp.vtex.com/navigation.json

# Alternative environment variable name
# NAVIGATION_URL=https://newhelp.vtex.com/navigation.json

# Default output path for downloaded files
DEFAULT_OUTPUT_PATH=./navigation.json

# Auto-format JSON output (true/false)
AUTO_FORMAT_JSON=true

# HTTP request timeout in milliseconds (default: 30000)
REQUEST_TIMEOUT=30000
